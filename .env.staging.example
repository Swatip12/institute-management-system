# Staging Environment Configuration

# Database Configuration
STAGING_DATABASE_URL=jdbc:postgresql://staging-db:5432/institute_staging_db
STAGING_DATABASE_USERNAME=institute_staging_user
STAGING_DATABASE_PASSWORD=staging_password_here

# Security Configuration
STAGING_JWT_SECRET=staging_jwt_secret_key_here

# API Configuration
API_URL=https://staging-api.yourdomain.com/api
PUBLIC_API_URL=https://staging-api.yourdomain.com/api/public

# Frontend Configuration
ENABLE_ANALYTICS=true
ENABLE_SERVICE_WORKER=false
LOG_LEVEL=warn
ENABLE_ERROR_TRACKING=true

# Monitoring Configuration
MONITORING_ENABLED=true
SENTRY_DSN=your_staging_sentry_dsn_here

# Email Configuration (for contact forms)
SMTP_HOST=smtp.yourdomain.com
SMTP_PORT=587
SMTP_USERNAME=staging-noreply@yourdomain.com
SMTP_PASSWORD=your_staging_smtp_password_here